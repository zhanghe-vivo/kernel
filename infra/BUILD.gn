import("//build/templates/build_template.gni")
build_rust("blue_infra") {
  crate_type = "rlib"
  sources = [ "src/lib.rs" ]
  edition = "2021"

  deps = [
    "//external/pinned-init/v0.0.8:pinned_init",
    "//external/cfg-if/v1.0.0:cfg_if",
    "//external/memchr/v2.7.4:memchr",
  ]

  features = [
    "compat_newlibc",
  ]
}
